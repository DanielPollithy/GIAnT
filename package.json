{
  "name": "transliteration_application",
  "productName": "Transliteration Application",
  "version": "0.0.3",
  "description": "A graphical transliteration tool based on mxgraph, Neo4J and electron",
  "main": "src/atom.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000",
    "docs": "yuidoc -c ./yuidoc.json src/. ",
    "server": "node src/server.js",
    "release": "node_modules/.bin/electron-zip-packager . "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielPollithy/TransliterationApplication.git"
  },
  "keywords": [
    "node",
    "electron",
    "mxgraph",
    "neo4j",
    "transliteration",
    "travis-ci",
    "yuidocs",
    "appveyor"
  ],
  "author": "Daniel Pollithy",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DanielPollithy/TransliterationApplication/issues"
  },
  "homepage": "https://github.com/DanielPollithy/TransliterationApplication#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "electron": "^1.6.7",
    "electron-packager": "^8.7.0",
    "electron-zip-packager": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "electron-log": "^2.2.6",
    "exif": "^0.6.0",
    "express": "^4.15.2",
    "express-fileupload": "^0.1.2",
    "image-size": "^0.5.4",
    "mxgraph": "^3.7.2-2",
    "neo4j-driver": "^1.1.1",
    "pug": "^2.0.0-beta11",
    "xml2js": "^0.4.17",
    "xmlhttprequest": "^1.8.0"
  }
}
